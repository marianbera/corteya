{
  "expo": {
    "name": "CorteYa",
    "slug": "Corteya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tuusuario.corteya"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tuusuario.corteya"
    },

    "web": {
      "favicon": "./assets/logo.png"
    },

    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Karla-Regular.ttf",
            "./assets/fonts/Karla-Bold.ttf",
            "./assets/fonts/Karla-Italic.ttf",
            "./assets/fonts/Karla-Light.ttf",
            "./assets/fonts/PressStart2P-Regular.ttf",
            "./assets/fonts/Mulidey.ttf"
          ]
        }
      ]
    ]
  }
}
